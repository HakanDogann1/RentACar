@model RentACar.EntityLayer.Concrete.Reference
@{
    ViewData["Title"] = "AddReference";
    Layout = "~/Views/AdminLayoutContoller/Index.cshtml";
}

<br />
<br />
<br />
<div class="card">
    <div class="card-body">
        <form method="post">
            <div class="mb-3">
                <label class="form-label">Başlık</label>
                <input type="text" class="form-control" asp-for="NameSurname">
            </div>
            <div class="mb-3">
                <label class="form-label">MEslek</label>
                <input type="text" class="form-control" asp-for="Job">
            </div>
            <div class="mb-3">
                <label class="form-label">Resim Url</label>
                <input type="text" class="form-control" asp-for="ImageUrl">
            </div>
            <div class="mb-3">
                <label class="form-label">Mesaj</label>
                <input type="text" class="form-control" asp-for="Message">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>

