@model List<RentACar.EntityLayer.Concrete.ContactMe>

    @foreach (var item in Model)
{
    <section class="ftco-section ftco-intro" style="background-image: url(@item.ContactMeImageUrl);">
        <div class="overlay"></div>
        <div class="container">
            <div class="row justify-content-end">
                <div class="col-md-6 heading-section heading-section-white ftco-animate">
                    <h2 class="mb-3">@item.ContactMeTitle</h2>
                    <a href="#" class="btn btn-primary btn-lg">Sürücü Ol</a>
                </div>
            </div>
        </div>
    </section>
}
