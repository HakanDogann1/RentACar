@model RentACar.EntityLayer.Concrete.ContactMe
@{
    ViewData["Title"] = "UpdateContactMe";
    Layout = "~/Views/AdminLayoutContoller/Index.cshtml";
}


<br />
<br />
<br />
<div class="card">
    <div class="card-body">
        <form method="post">
            <input type="hidden" asp-for="ContactMeID" />
            <div class="mb-3">
                <label class="form-label">Başlık</label>
                <input type="text" class="form-control" asp-for="ContactMeTitle">
            </div>
            <div class="mb-3">
                <label class="form-label">Resim Url</label>
                <input type="text" class="form-control" asp-for="ContactMeImageUrl">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>


