@model RentACar.EntityLayer.Concrete.TagCloud
@{
    ViewData["Title"] = "UpdateTagCloud";
    Layout = "~/Views/AdminLayoutContoller/Index.cshtml";
}

<br />
<br />
<br />
<div class="card">
    <div class="card-body">
        <form method="post">
            <input type="hidden" asp-for="TagCloudID" />
            <div class="mb-3">
                <label class="form-label">Başlık</label>
                <input type="text" class="form-control" asp-for="TagCloudTitle">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>

