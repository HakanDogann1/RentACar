@model RentACar.EntityLayer.Concrete.AboutUs
@{
    ViewData["Title"] = "UpdateAboutUs";
    Layout = "~/Views/AdminLayoutContoller/Index.cshtml";
}
<br />
<br />
<br />
<div class="card">
    <div class="card-body">
        <form method="post">
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Başlık</label>
                <input type="text" class="form-control" id="exampleInputTitle" asp-for="AboutUsTitle">
                
            </div>
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">İçerik</label>
                <textarea type="text" class="form-control" id="exampleInputdescription" asp-for="AboutUsDescription"></textarea>
            </div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Resim Url</label>
                <input type="text" class="form-control" id="exampleInputımage" asp-for="AboutUsImageUrl">

            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>
